{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "no-loops",
        "json",
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "jquery": true,
        "node": true
    },
    "globals": {
        "URI": false
    },
    "settings": {
        "import/resolver": {
            "babel-module": {
                "root": [
                    "./src"
                ]
            }
        }
    },
    "rules": {
        "react/no-find-dom-node": 0,
        "react/no-unknown-property": 2,
        "react/prefer-stateless-function": 2,
        "react/jsx-tag-spacing": [
            2,
            {
                "beforeSelfClosing": "always"
            }
        ],
        "react/jsx-max-props-per-line": [
            2,
            {
                "maximum": 1,
                "when": "multiline"
            }
        ],
        "react/jsx-no-bind": 2,
        "react/jsx-no-target-blank": 2,
        "react/jsx-wrap-multilines": 2,
        "react/jsx-pascal-case": 2,
        "no-loops/no-loops": 2,
        "import/first": 1,
        "import/prefer-default-export": 2,
        "import/no-duplicates": 2,
        "import/export": 2,
        "import/extensions": [
            2,
            "always",
            {
                "js": "never"
            }
        ],
        "no-alert": 1,
        "no-array-constructor": 2,
        "no-arrow-condition": 0,
        "no-bitwise": 0,
        "no-caller": 0,
        "no-case-declarations": 2,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-confusing-arrow": [
            2,
            {
                "allowParens": true
            }
        ],
        "no-console": 1,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 0,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-label": 0,
        "no-empty-pattern": 2,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 0,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "all",
            {
                "ignoreJSX": "all"
            }
        ],
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 0,
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-inner-declarations": [
            0,
            "both"
        ],
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 0,
        "no-lone-blocks": 2,
        "no-lonely-if": 0,
        "no-loop-func": 2,
        "no-mixed-requires": [
            0,
            {
                "grouping": false
            }
        ],
        "no-mixed-spaces-and-tabs": [
            2,
            false
        ],
        "no-mixed-operators": [
            2,
            {
                "allowSamePrecedence": true
            }
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "no-multi-spaces": [
            2,
            {
                "exceptions": {
                    "Property": true,
                    "VariableDeclarator": true,
                    "ImportDeclaration": true
                }
            }
        ],
        "no-multi-str": 2,
        "no-multiple-empty-lines": [
            2,
            {
                "max": 2,
                "maxEOF": 1,
                "maxBOF": 0
            }
        ],
        "no-native-reassign": 2,
        "no-negated-condition": 0,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-path-concat": 0,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 0,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": [
            2,
            "except-parens"
        ],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 0,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-sync": 0,
        "no-ternary": 0,
        "no-template-curly-in-string": 1,
        "no-trailing-spaces": 2,
        "no-this-before-super": 2,
        "no-throw-literal": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 0,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 0,
        "no-unmodified-loop-condition": 0,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unsafe-negation": 2,
        "no-unused-expressions": [
            2,
            {
                "allowShortCircuit": true
            }
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used",
                "argsIgnorePattern": "err|req|res|next"
            }
        ],
        "no-use-before-define": 0,
        "no-useless-call": 0,
        "no-useless-concat": 2,
        "no-useless-return": 2,
        "no-void": 0,
        "no-var": 2,
        "no-warning-comments": [
            0,
            {
                "terms": [
                    "todo",
                    "fixme",
                    "xxx"
                ],
                "location": "start"
            }
        ],
        "no-whitespace-before-property": 2,
        "no-with": 2,
        "no-magic-numbers": [
            1,
            {
                "ignore": [
                    -1,
                    0,
                    1
                ],
                "ignoreArrayIndexes": true
            }
        ],
        "array-bracket-spacing": [
            2,
            "always",
            {
                "arraysInArrays": false,
                "objectsInArrays": true,
                "singleValue": false
            }
        ],
        "array-callback-return": 0,
        "arrow-body-style": [
            0,
            "as-needed"
        ],
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "accessor-pairs": 2,
        "block-scoped-var": 1,
        "block-spacing": 0,
        "brace-style": [
            2,
            "1tbs"
        ],
        "callback-return": 0,
        "camelcase": 0,
        "comma-dangle": [
            2,
            "never"
        ],
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            2,
            "last"
        ],
        "complexity": [
            0,
            11
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "consistent-return": 0,
        "consistent-this": [
            1,
            "self"
        ],
        "constructor-super": 2,
        "curly": [
            2,
            "multi-line",
            "consistent"
        ],
        "default-case": 2,
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": [
            0,
            {
                "allowKeywords": true
            }
        ],
        "eol-last": 2,
        "eqeqeq": [
            2,
            "always",
            {
                "null": "ignore"
            }
        ],
        "func-call-spacing": [
            2,
            "never"
        ],
        "func-names": 0,
        "func-style": [
            2,
            "declaration",
            {
                "allowArrowFunctions": true
            }
        ],
        "generator-star-spacing": [
            2,
            "after"
        ],
        "global-require": 0,
        "guard-for-in": 2,
        "handle-callback-err": 0,
        "id-length": 0,
        "indent": [
            2,
            4,
            {
                "MemberExpression": 0,
                "SwitchCase": 1,
                "VariableDeclarator": 2
            }
        ],
        "init-declarations": 0,
        "jsx-quotes": [
            2,
            "prefer-double"
        ],
        "key-spacing": [
            2,
            {
                "beforeColon": false,
                "afterColon": true,
                "mode": "minimum"
            }
        ],
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true,
                "overrides": {}
            }
        ],
        "lines-around-comment": 0,
        "max-depth": [
            2,
            4
        ],
        "max-len": [
            2,
            100,
            4
        ],
        "max-nested-callbacks": [
            0,
            2
        ],
        "max-params": [
            0,
            3
        ],
        "max-statements": [
            0,
            10
        ],
        "new-cap": [
            2,
            {
                "newIsCap": true,
                "capIsNew": false
            }
        ],
        "new-parens": 2,
        "padding-line-between-statements": [
            2,
            {
                "blankLine": "always",
                "prev": [
                    "const",
                    "let",
                    "var"
                ],
                "next": "*"
            },
            {
                "blankLine": "any",
                "prev": [
                    "const",
                    "let",
                    "var"
                ],
                "next": [
                    "const",
                    "let",
                    "var"
                ]
            }
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "object-curly-newline": [
            2,
            {
                "ObjectExpression": {
                    "multiline": true,
                    "consistent": true
                }
            }
        ],
        "object-property-newline": [
            2,
            {
                "allowMultiplePropertiesPerLine": true
            }
        ],
        "object-shorthand": [
            2,
            "consistent"
        ],
        "one-var": [
            2,
            {
                "initialized": "never",
                "uninitialized": "always"
            }
        ],
        "operator-assignment": [
            0,
            "always"
        ],
        "operator-linebreak": [
            2,
            "before"
        ],
        "padded-blocks": [
            2,
            "never"
        ],
        "prefer-arrow-callback": 0,
        "prefer-const": [
            2,
            {
                "destructuring": "any",
                "ignoreReadBeforeAssign": true
            }
        ],
        "prefer-reflect": 0,
        "prefer-rest-params": 0,
        "prefer-spread": 0,
        "prefer-template": 2,
        "quote-props": 0,
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "radix": 2,
        "rest-spread-spacing": [
            2,
            "never"
        ],
        "id-match": 0,
        "require-jsdoc": 0,
        "require-yield": 0,
        "semi": [
            2,
            "always"
        ],
        "semi-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "sort-vars": 0,
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            "never"
        ],
        "space-in-parens": [
            2,
            "never"
        ],
        "space-infix-ops": 2,
        "space-unary-ops": [
            2,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "spaced-comment": [
            2,
            "always",
            {
                "block": {
                    "markers": [
                        "*"
                    ],
                    "balanced": true
                }
            }
        ],
        "strict": [
            0,
            "global"
        ],
        "template-tag-spacing": [
            2,
            "never"
        ],
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": [
            2,
            "inside"
        ],
        "wrap-regex": 0,
        "yoda": [
            2,
            "never"
        ]
    }
}
